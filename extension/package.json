{
  "name": "mobile-vscode-server",
  "displayName": "Mobile VSCode Server",
  "publisher": "mobilevs",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": ["Other"],
  "main": "./out/extension.js",
  "activationEvents": [
    "onCommand:mobile-vscode-server.start"
  ],
  "contributes": {
    "commands": [
      {
        "command": "mobile-vscode-server.start",
        "title": "Start Mobile VSCode Server"
      }
    ]
  },
  "scripts": {
    "build": "tsc -p . && yarn workspace backend build && node ./copyBackend.cjs",
    "vscode:prepublish": "npm run build"
  },
  "dependencies": {
    "apollo-server-express": "^3.10.4",
    "chokidar": "^3.5.3",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.11.0",
    "jsonwebtoken": "^9.0.0",
    "lodash.debounce": "^4.0.8",
    "simple-git": "^3.19.0",
    "typescript-language-server": "^0.11.2",
    "vscode-jsonrpc": "^8.1.0",
    "vscode-node-debug2": "github:microsoft/vscode-node-debug2#v1.43.0",
    "ws": "^8.12.0",
    "y-websocket": "^1.5.0"
  },
  "devDependencies": {
    "typescript": "^5.1.3"
  }
}
