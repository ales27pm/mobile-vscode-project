{
  "name": "mobile-vscode-server",
  "displayName": "Mobile VSCode Server",
  "publisher": "codex",
  "version": "0.1.0",
  "description": "Provides GraphQL, Git and Y.js back-end for the Mobile VSCode app.",
  "main": "./dist/extension.js",
  "scripts": {
    "prepublish": "yarn build",
    "build": "tsc -p .",
    "bundle": "webpack --config webpack.config.js",
    "watch": "tsc -p . -w"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "activationEvents": [
    "onCommand:mobile-vscode-server.start"
  ],
  "contributes": {
    "commands": [
      {
        "command": "mobile-vscode-server.start",
        "title": "Start Mobile VSCode Server"
      },
      {
        "command": "mobile-vscode-server.stop",
        "title": "Stop Mobile VSCode Server"
      }
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@graphql-tools/schema": "^8.3.0",
    "apollo-server-express": "^3.13.0",
    "cors": "^2.8.5",
    "express": "^4.22.1",
    "graphql": "^16.6.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^6.0.6",
    "lodash.debounce": "^4.0.8",
    "simple-git": "^3.30.0",
    "ws": "^8.13.0",
    "y-websocket": "1.5.3",
    "yjs": "^13.6.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/node": "^16.18.126",
    "@types/vscode": "^1.107.0",
    "@types/ws": "^8.18.1",
    "ts-loader": "^9.5.4",
    "typescript": "^4.9.5",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.9.2"
  }
}
