{
  "name": "mobile-vscode-server",
  "displayName": "Mobile VSCode Server",
  "publisher": "codex",
  "version": "0.1.0",
  "description": "Provides GraphQL, Git and Y.js back-end for mobile client",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "main": "dist/index.js",
  "activationEvents": [
    "onCommand:mobile-vscode-server.start"
  ],
  "contributes": {
    "commands": [
      {
        "command": "mobile-vscode-server.start",
        "title": "Start Mobile VSCode Server"
      }
    ]
  },
  "scripts": {
    "build": "tsc -b",
    "start": "node dist/index.js",
    "test": "jest --passWithNoTests",
    "vscode:prepublish": "yarn build"
  },
  "dependencies": {
    "apollo-server-express": "^3.10.4",
    "chokidar": "^3.5.3",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tools": "^8.3.20",
    "graphql-ws": "^5.11.0",
    "jsonwebtoken": "^9.0.0",
    "lodash.debounce": "^4.0.8",
    "simple-git": "^3.19.0",
    "typescript-language-server": "^0.11.2",
    "vscode-jsonrpc": "^8.1.0",
    "ws": "^8.12.0",
    "y-websocket": "^1.5.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "^18.16.18",
    "@types/ws": "^8.18.1",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  }
}
